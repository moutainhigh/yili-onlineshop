<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 命名空间 -->
	<typeAliases>
		<!-- 门店商品信息 -->
		<typeAlias alias="StoreSku" type="com.aladingshop.store.sku.model.StoreSku" />
		<!-- 门店商品图片信息 -->
		<typeAlias alias="StoreSkuImage" type="com.aladingshop.store.sku.model.StoreSkuImage" />
		<!-- 门店产品属性信息 -->
		<typeAlias alias="StoreSkuProductAttr" type="com.aladingshop.store.sku.model.StoreSkuProductAttr" />
		<!-- 门店商品图片信息 -->
		<typeAlias alias="StoreSkuProductImage" type="com.aladingshop.store.sku.model.StoreSkuProductImage" />
		<!-- 门店产品信息 -->
		<typeAlias alias="StoreSkuProduct" type="com.aladingshop.store.sku.model.StoreSkuProduct" />
		<!-- 门店库存信息 -->
		<typeAlias alias="StoreSkuInventory" type="com.aladingshop.store.sku.model.StoreSkuInventory" />
		<!-- 商品价格信息 -->
		<typeAlias alias="StoreSkuPrice" type="com.aladingshop.store.sku.model.StoreSkuPrice" />
	</typeAliases>

	<!-- 注册SQL映射文件 -->
	<mappers>
	</mappers>
</configuration>