<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 命名空间 -->
	<typeAliases>
		<!--门店角色 -->
		<typeAlias alias="StoreAuthRole" type="com.aladingshop.store.auth.model.StoreAuthRole" />
		<!-- 门店权限-->
		<typeAlias alias="StoreAuthPrivilege" type="com.aladingshop.store.auth.model.StoreAuthPrivilege" />
		<!-- 门店后台用户  -->
		<typeAlias alias="StoreAuthUser" type="com.aladingshop.store.auth.model.StoreAuthUser" />
		<!-- 门店后台角色权限中间表  -->
		<typeAlias alias="StoreAuthRolePrivilege" type="com.aladingshop.store.auth.model.StoreAuthRolePrivilege" />
		<!--门店后台用户角色中间表  -->
		<typeAlias alias="StoreAuthUserRole" type="com.aladingshop.store.auth.model.StoreAuthUserRole" />
	</typeAliases>
	<!-- 注册SQL映射文件 -->
	<mappers>
	</mappers>
</configuration>